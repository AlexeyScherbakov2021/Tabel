<Window x:Class="Tabel.Views.AddFormStringWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Tabel.Views"
        mc:Ignorable="d"
        Height="220" Width="406" WindowStartupLocation="CenterScreen" Background="#FFECECEC">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="85"/>
            <RowDefinition/>
            <RowDefinition Height="35.007"/>
        </Grid.RowDefinitions>
        <TextBlock Text="Наименование" Margin="20,0,0,6" VerticalAlignment="Bottom"/>
        <TextBox Name="resultText" Grid.Row="1" Margin="10,3,10,0" VerticalContentAlignment="Center" VerticalAlignment="Top" TabIndex="-1" AutomationProperties.IsRequiredForForm="True"/>
        <UniformGrid Margin="0,1,0,0" Grid.Row="2" Columns="2">
            <Button Content="OK" Margin="3" IsDefault="True" Click="Button_Click"/>
            <Button Content="Cancel" Margin="3" IsCancel="True"/>
        </UniformGrid>
    </Grid>
</Window>
